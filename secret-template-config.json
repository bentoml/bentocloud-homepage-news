[
    {
        "name": "OPENAI",
        "description": "grant access to your OPENAI API",
        "icon": "https://raw.githubusercontent.com/bentoml/bentocloud-homepage-news/bentocloud/secrets/imgs/chatgpt.svg",
        "type": "env",
        "content": {
            "items": [
                {
                    "key": "OPENAI_API_KEY"
                }
            ]
        }
    },
    {
        "name": "AWS",
        "description": "access your AWS resources, e.g, s3 storage",
        "icon": "https://raw.githubusercontent.com/bentoml/bentocloud-homepage-news/bentocloud/secrets/imgs/aws.svg",
        "type": "env",
        "content": {
            "items": [
                {
                    "key": "AWS_ACCESS_KEY_ID"
                },
                {
                    "key": "AWS_SECRET_ACCESS_KEY"
                },
                {
                    "key": "AWS_REGION"
                }
            ]
        }
    },
    {
        "name": "Hugging Face",
        "description": "require your token to use the models in Hugging Face",
        "icon": "https://raw.githubusercontent.com/bentoml/bentocloud-homepage-news/bentocloud/secrets/imgs/hf.svg",
        "type": "env",
        "content":{
            "items": [
                {
                    "key": "HF_TOKEN"
                }
            ]
        }
    },
    {
        "name": "Github Token",
        "description": "access your Github resources, e.g, private repositories",
        "icon": "https://raw.githubusercontent.com/bentoml/bentocloud-homepage-news/bentocloud/secrets/imgs/github.svg",
        "type": "env",
        "content":{
            "items": [
                {
                    "key": "GITHUB_TOKEN"
                }
            ]
        }
    }
]